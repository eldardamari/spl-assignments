#ifndef STUDEND_H_
#define STUDEND_H_

class Student{

protected:
		const string _studentId;
		const string _imagePath;
		unsigned short _unfinishedSemsterCourses;
		unsigned short _currentSemster;
		unsigned short _electiveCoursesUnfinishd;

		Student(){};

public:
		void finishcourse();
		virtual void study(Course &c)=0;
		virtual ~Student(){ std::cout << "Student is dead!" << endl;}

};

#endif
